#pragma rtGlobals=1		// Use modern global access method.Function AppendToLog(nb,str,stampDateTime)	String nb	String str	Variable stampDateTime		Silent 1		Variable/D now	String stamp		Notebook $nb selection={endOfFile,endOfFile}	if(stampDateTime)		now=datetime		stamp=Secs2Date(now,0)+", " + Secs2Time(now,0) +"\r"		Notebook $nb text=stamp	endif	NoteBook $nb text= str+"\r"End