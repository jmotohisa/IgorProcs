#pragma rtGlobals=3		// Use modern global access method.Function/S wname(name)	string name	variable start,stop,length,tmp=-1	length=strlen(name)	do		start=tmp+1		tmp=strsearch(name,":",start)	while(tmp>-1)	stop=strsearch(name,".",0 )	if (stop<0)		stop=length	endif	return name[start,stop-1]ENDFunction/S fnameToWname(path)	String path	return ParseFilePath(3, path, ":", 0, 0)EndFunction/S basename(path)	String path	return ParseFilePath(0, path, ":", 1, 0)EndFunction/S dirname(path)	String path	return(ParseFilePath(1, path, ":", 1, 0))EndFunction/S ParentPath(path)	String path	Variable index	return basename(dirname(path))//	String folder//	index=ItemsInList(path,":")//	folder=StringFromList(index-1,path,":")//	return folderEnd	