#pragma rtGlobals=1		// Use modern global access method.// MaterialParameters.ipf// Parameters for elemental and binary semiconductors// and functions for ternary alloys// see the references in the bottom of this file // // materials supported: GaAs, InP, InAs, AlAs, GaP////	05/12/21 ver. 0.1c by J. Motohisa////	revision history//		05/12/01 ver. 0.1a first version, parameters for GaAs, InAs, InP//		05/12/?? ver. 0.1b parameters for AlAs added (not incomplete)//		05/12/21 ver. 0.1c parameters for GaP added (not incomplete)//		08/05/10 ver 0.1c1 init_materials modified//		08/07/27 ver 0.1c2 parameters 'ems_DOS_CB' and 'ems_DOS_VB' added (not imcomplete though)//		08/09/12 ver 0.1c3 parameters 'Edonor', 'g_D' and etc. added (not imcomplete though)//		10/12/09 ver 0.2a parameters for WZ are partly added, GaN, AlN, and InNProc init_materials()	PauseUpdate;Silent 1	String cmd,material_list	material_list="param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP,param_GaN, param_AlN, param_InN"// make waves	cmd="Make/O/D/N=24 "+material_list;execute cmd//	Make/O/D/N=15 param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP//set dim labels	cmd="SetDimLabel 0,0,'a0', "+material_list;execute cmd //SetDimLabel 0,0,'a0',   param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // lattice constant, A	cmd="SetDimLabel 0,1,'c11', "+material_list;execute cmd // param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // elastic stiffness, 10^12 dyn/cm^2	cmd="SetDimLabel 0,2,'c12', "+material_list;execute cmd // param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // elastic stiffness, 10^12 dyn/cm^2	cmd="SetDimLabel 0,3,'c44', "+material_list;execute cmd // param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // elastic stiffness, 10^12 dyn/cm^2	cmd="SetDimLabel 0,4,'ac', "+material_list;execute cmd //  param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // deformation potential for conduction band, eV	cmd="SetDimLabel 0,5,'av', "+material_list;execute cmd //  param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // deformation potential for valence band, eV	cmd="SetDimLabel 0,6,'b' , "+material_list;execute cmd //  param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // shear deformation potential, eV	cmd="SetDimLabel 0,7,'d' "+material_list;execute cmd //   ,param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // ??? deformation potential, eV	cmd="SetDimLabel 0,8,'delta', "+material_list;execute cmd //param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // spin-orbit splitting, eV	cmd="SetDimLabel 0,9,'Evavr', "+material_list;execute cmd //param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // average valence band energy in MST, eV	cmd="SetDimLabel 0,10,'Eg', "+material_list;execute cmd // param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // band gap, eV (at 0K, use of "Bandgaps.ipf" is recommended)	cmd="SetDimLabel 0,11,'g1', "+material_list;execute cmd // param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // Luttinger parameters	cmd="SetDimLabel 0,12,'g2', "+material_list;execute cmd // param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // Luttinger parameters	cmd="SetDimLabel 0,13,'g3', "+material_list;execute cmd // param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // Luttinger parameters	cmd="SetDimLabel 0,14,'ems', "+material_list;execute cmd // param_GaAs,param_InAs,param_InP,param_AlAs,param_GaP // effective mass of electrons at Gamma point (unless otherwise specified), m0	cmd="SetDimLabel 0,15,'ems_DOS_CB', "+material_list;execute cmd // density of states effective mass of CB	cmd="SetDimLabel 0,16,'ems_DOS_VB', "+material_list;execute cmd // density of states effective mass of Vb	cmd="SetDimLabel 0,17,'Edonor', "+material_list;execute cmd // donor level	cmd="SetDimLabel 0,18,'Eacceptor', "+material_list;execute cmd // acceptor level	cmd="SetDimLabel 0,19,'g_D', "+material_list;execute cmd // degeneracy factor of donor level	cmd="SetDimLabel 0,20,'g_A', "+material_list;execute cmd // degeneracy factor of acceptor level	cmd="SetDimLabel 0,21,'c0', "+material_list;execute cmd //lattice constant of c-axis (WZ)	cmd="SetDimLabel 0,22,'c13', "+material_list;execute cmd //elastic stiffness (WZ), currently GPa	cmd="SetDimLabel 0,23,'c33', "+material_list;execute cmd //elastic stiffness (WZ), currently GPa	// parameters for GaAs	param_GaAs[%'a0']  = 5.6533;	param_GaAs[%'c11'] = 1.223 ; param_GaAs[%'c12'] = 0.571; param_GaAs[%'c44'] = 0.600	param_GaAs[%'ac']  = -7.17 ; param_GaAs[%'av']  = 1.16 ; param_GaAs[%'b'] = -1.90 ; param_GaAs[%'d'] = -4.23	param_GaAs[%'delta'] = 0.34; param_GaAs[%'Evavr'] = -6.92; param_GaAs[%'Eg'] = 1.5192	param_GaAs[%'g1'] = 6.85; param_GaAs[%'g2'] = 2.1; param_GaAs[%'g3'] = 2.9//	param_GaAs[%'g1'] = 7.65; param_GaAs[%'g2'] = 2.41; param_GaAs[%'g3'] = 3.28 // ref (4)	param_GaAs[%'ems']= 0.067;param_GaAs[%'ems_DOS_CB']=0.067;param_GaAs[%'ems_DOS_VB']=0.47	param_GaAs[%'Edonor']= 0.006;param_GaAs[%'g_D']=2;param_GaAs[%'Eacceptor']= 0.026;param_GaAs[%'g_A']=4// parameters for InAs	param_InAs[%'a0']  = 6.0584;	param_InAs[%'c11'] = 0.833 ; param_InAs[%'c12'] = 0.453; param_InAs[%'c44'] = 0.396	param_InAs[%'ac']  = -5.08 ; param_InAs[%'av']  = 1.00 ; param_InAs[%'b'] = -1.55 ; param_InAs[%'d'] = -3.10	param_InAs[%'delta'] = 0.38; param_InAs[%'Evavr'] = -6.67; param_InAs[%'Eg'] = 0.41	param_InAs[%'g1'] = 20.4; param_InAs[%'g2'] = 8.3; param_InAs[%'g3'] = 9.1//	param_InAs[%'g1'] = 19.67; param_InAs[%'g2'] = 8.37; param_InAs[%'g3'] = 9.29// ref (4)	param_InAs[%'ems']= 0.027// parameters for InP	param_InP[%'a0']  = 5.8688;	param_InP[%'c11'] = 1.022 ; param_InP[%'c12'] = 0.576; param_InP[%'c44'] = 0.460	param_InP[%'ac']  = -5.04 ; param_InP[%'av']  = 1.27 ; param_InP[%'b'] = -1.60 ; param_InP[%'d'] = -4.20	param_InP[%'delta'] = 0.11; param_InP[%'Evavr'] = -7.04; param_InP[%'Eg'] = 1.42	param_InP[%'g1'] = 4.95; param_InP[%'g2'] = 1.64; param_InP[%'g3'] = 2.35//	param_InP[%'g1'] = 6.28; param_InP[%'g2'] = 2.08; param_InP[%'g3'] = 2.76 // ref (4)	param_InP[%'ems']= 0.077// parameters for AlAs % not all the parameters are updated; b,d,g1,g2,g3	param_AlAs[%'a0']  = 5.6611;	param_AlAs[%'c11'] = 1.250 ; param_AlAs[%'c12'] = 0.534; param_AlAs[%'c44'] = 0.542	param_AlAs[%'ac']  = -5.64 ; param_AlAs[%'av']  = 2.47 ; param_AlAs[%'b'] = 0 ; param_AlAs[%'d'] = 0	param_AlAs[%'delta'] = 0.28; param_AlAs[%'Evavr'] = -7.49; param_AlAs[%'Eg'] = 3.13	param_AlAs[%'g1'] = 4.04; param_AlAs[%'g2'] = 0.78; param_AlAs[%'g3'] = 1.57 //  Ref (4)	param_AlAs[%'ems']= 0.15// parameters for GaP % not all the parameters are updated; b,d,g1,g2,g3,ems	param_GaP[%'a0']  = 5.4505;	param_GaP[%'c11'] = 1.439 ; param_GaP[%'c12'] = 0.652; param_GaP[%'c44'] = 0.714	param_GaP[%'ac']  = -7.14 ; param_GaP[%'av']  = 1.70 ; param_GaP[%'b'] = -1.5 ; param_GaP[%'d'] = -4.6	param_GaP[%'delta'] = 0.08; param_GaP[%'Evavr'] = -7.40; param_GaP[%'Eg'] = 2.90	param_GaP[%'g1'] = 4.20; param_GaP[%'g2'] = 0.98; param_GaP[%'g3'] = 1.66 // Ref 4	param_GaP[%'ems']= 0.17// parameters for WZ GaN % not all the parameters are updated; b,d,g1,g2,g3,ems	param_GaN[%'a0']  = 3.189;	param_GaN[%'c11'] = 390 ; param_GaN[%'c12'] = 145; param_GaN[%'c13'] = 106;param_GaN[%'c33'] = 398;param_GaN[%'c44'] = 105		// units is GPa, may be different from ZB materials//	param_GaN[%'ac']  = -7.14 ; param_GaN[%'av']  = 1.70 ; param_GaN[%'b'] = -1.5 ; param_GaN[%'d'] = -4.6//	param_GaN[%'delta'] = 0.08; param_GaN[%'Evavr'] = -7.40; param_GaN[%'Eg'] = 2.90//	param_GaN[%'g1'] = 4.20; param_GaN[%'g2'] = 0.98; param_GaN[%'g3'] = 1.66//	param_GaN[%'ems']= 5.185	param_GaN[%'c0']= 5.185// parameters for WZ AlN % not all the parameters are updated; b,d,g1,g2,g3,ems	param_AlN[%'a0']  = 3.112;	param_AlN[%'c11'] = 396 ; param_AlN[%'c12'] = 137; param_AlN[%'c13'] = 108;param_AlN[%'c33'] = 373;param_AlN[%'c44'] = 116		// units is GPa, may be different from ZB materials//	param_AlN[%'ac']  = -7.14 ; param_AlN[%'av']  = 1.70 ; param_AlN[%'b'] = -1.5 ; param_AlN[%'d'] = -4.6//	param_AlN[%'delta'] = 0.08; param_AlN[%'Evavr'] = -7.40; param_AlN[%'Eg'] = 2.90//	param_AlN[%'g1'] = 4.20; param_AlN[%'g2'] = 0.98; param_AlN[%'g3'] = 1.66//	param_AlN[%'ems']= 5.185	param_AlN[%'c0']= 4.982// parameters for WZ InN % not all the parameters are updated; b,d,g1,g2,g3,ems	param_InN[%'a0']  = 3.545;	param_InN[%'c11'] = 223 ; param_InN[%'c12'] = 115; param_InN[%'c13'] = 92;param_InN[%'c33'] = 224;param_InN[%'c44'] = 48		// units is GPa, may be different from ZB materials//	param_InN[%'ac']  = -7.14 ; param_InN[%'av']  = 1.70 ; param_InN[%'b'] = -1.5 ; param_InN[%'d'] = -4.6//	param_InN[%'delta'] = 0.08; param_InN[%'Evavr'] = -7.40; param_InN[%'Eg'] = 2.90//	param_InN[%'g1'] = 4.20; param_InN[%'g2'] = 0.98; param_InN[%'g3'] = 1.66//	param_InN[%'ems']= 5.185	param_InN[%'c0']= 5.703End////////////////////////// Functions to calculate parameters in ternary alloys (with and without bowing)Function/D ParamTernAlloy(xx,aa1,aa2,bow)	Variable xx,aa1,aa2,bow	return(aa1*xx+aa2*(1-xx)+bow*xx*(xx-1))EndFunction/D ParamTernAlloy0(xx,aa1,aa2)	Variable xx,aa1,aa2	return(aa1*xx+aa2*(1-xx))End// elastic stiffness in III1_{x}-III2_{1-x}-V alloy: Function/D ElasticStiffnessTernAlloy(xx,cc1,aa1,cc2,aa2)	Variable xx,cc1,aa1,cc2,aa2	return ((xx*cc1*aa1+(1-xx)*cc2*aa2)/(aa1*xx+aa2*(1-xx)))End//	References and Nots// (0) Landold-B\"ornstein// (1) C. van de Walle, PRB 39, 1871 (1989).// (2) C.Y.-P. Chao and S.L.Chuang, PRB 46, 4110 (1989).// (3) M. Sugawara, N. Okazaki, T. Fujii, and S. Yamazaki, PRB 48l, 8102 (1993).// (4) P. Lawaetz, Phys. Rev. B4, 3460 (1971). //// Lattice constant of InAs: In ref (1), a0=6.08, but in (2) and (3), a0=6.0584, giving difference of 0.3%// deformation potential b and d for InAs: theoretical values: -1.55 and -3.10 (ref (1)), exp values -1.8 and -3.6 (ref (0))