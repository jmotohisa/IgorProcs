Macro CalcProfile(waveName,area,epsilon)	String waveName	Variable/D area=0.23,epsilon=13.1	Prompt waveName,"wave name of C",popup,WaveList("*",";","")	Prompt area,"area (mm squre)"	Prompt epsilon,"epsilon"	PauseUpdate; Silent 1	String dC_2dV,wdep,profile	dC_2dV = waveName + "_dC_2dV"	wdep = waveName + "_Wdep"	profile = waveName + "_prof"	Duplicate/O $waveName $dC_2dV,$wdep,$profile		area = area * 1e-6	epsilon = epsilon * 8.85e-12		$dC_2dV = 1/$waveName^2	Differentiate $dC_2dV	$Profile = -2/$dC_2dV/1.602e-19/epsilon/area^2	$wdep = 1/$waveName * (epsilon) * area		SetScale y 0,0,"m", $wdep	SetScale y 0,0,"m3",$profile		Display $Profile vs $wdepEnd Macro