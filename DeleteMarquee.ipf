| DeleteMarqueeData() macro|	You can use the DeleteMarqueeData() macro to grapically select data points|	to delete from a wave.#include <Delete Marquee Points>		// in “WaveMetrics Procedures” folder#include <Strings as Lists>Menu "Macros"	"Initialize Deletion/1", InitDeletePointsFromWave()	"Delete Marquee Data from Wave/2", DoDeleteMarqueeData()	"-"EndFunction InitDeletePointsFromWave()		// run this once to create global variables used by DeleteMarqueeData	Variable/G gTestPlotTypeSave=2//	InitDeleteMarqueeData()				// in “Delete Marquee Points” utility fileEndMacro AllWaveStats()	PauseUpdate;	Silent 1	String wname	Variable index=0,indexm		do//		wname=GetStrFromList(list,index,";")		wname=WaveName("", index,4)		if(strlen(wname)==0)			break		endif		index +=1	while(1)	indexm=index		Make /O/N=(index)/R $"Ndata",$"Average",$"Stdev",$"rms"	Print "Statitics of",indexm,"waves in the current experiment."	index=0	do//		wname=GetStrFromList(list,index,";")		wname=WaveName("", index,4)		WaveStats/Q $wname		$"Ndata"[index] = V_npnts		$"Average"[index] = V_avg		$"Stdev"[index] = V_sdev		$"rms"[index] = V_rms		Print "Statistics of",wname,": V_npnts=",V_npnts,"V_avg=",V_avg,"V_sdev=",V_sdev,"V_rms=",V_rms		index +=1		indexm -=1	while(indexm>0)EndMacro	Window Table0() : Table	PauseUpdate; Silent 1		| building window...	Edit/W=(5,42,432,249) Ndata.y,Average.y,Stdev.y,rms.yEndMacro