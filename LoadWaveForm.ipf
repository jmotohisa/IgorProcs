#include <Strings as Lists>Macro LoadWFData(fileName,pathName,index)	String fileName	String pathName	Variable index		Silent 1; PauseUpDate		String w0,w1,w00,w11		LoadWave/G/D/A/W/P=$pathName fileName	if(V_flag==0)		return	endif		w0 = GetStrFromList(S_waveNames,0,";")	w1 = GetStrFromList(S_waveNames,1,";")	WaveStats/Q $w0	SetScale/I x,V_min,V_max,"sec",$w1	SetScale y,0,0,"V",$w1|	SetScale|	SetScale/I x,V_min,V_max,"V",$Id|	SetScale y,0,1,"I",$Id|	KillWaves $Vg	w00= "time" + num2istr(index)	w11 = "voltage" + num2istr(index)	Rename $w0,$w00	Rename $w1,$w11|	Display $w11EndMacro LoadAllWFData(pathName)	String pathName		Silent 1; PauseUpDate		String fileName	String w0,w1	Variable index=0		if(strlen(pathName)==0)		NewPath/O temporaryPath		PathName = "temporaryPath"	endif		do		fileName = IndexedFile($pathName, index,"TEXT")		if(strlen(fileName)==0)			break		endif		LoadWFData(fileName,pathName,index)|		if(strlen(S_waveNames)>0)|		endif		index +=1	while(1)		if(Exists("temporaryPath"))		KillPath temporaryPath	endifEnd