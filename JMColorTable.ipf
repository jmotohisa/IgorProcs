#pragma rtGlobals=1		// Use modern global access method.#include <KBColorizeTraces>Macro InitializeJMColortable(ncolor,lightness, saturation, startingHue)	Variable ncolor=21,lightness=1,saturation=1, startingHue=0.5	PauseUpdate;Silent 1;	Variable rmin, rmax, gmin, gmax, bmin,bmax, phi, r,g,b	Variable k, km	Variable icol	Make/O/N=(ncolor) JMcolortable_R,JMcolortable_G,JMcolortable_B	k = ncolor	if (k <= 0)		return -1	endif	km = k	icol=0	Do		k-=1		JMcolortable_R[k] = KBGetColor( lightness, saturation, (mod(startingHue + k/km, 1)), 1 )		JMcolortable_G[k] = KBGetColor( lightness, saturation, (mod(startingHue + k/km, 1)), 2 )		JMcolortable_B[k] = KBGetColor( lightness, saturation, (mod(startingHue + k/km, 1)), 3 )		icol+=1	while(k>0)EndFunction/S JMRGBcolor(index)	Variable index	String s	wave  JMcolortable_R,JMcolortable_G,JMcolortable_B	s="("+num2str(JMcolortable_R[index])+","+num2str(JMcolortable_G[index])+","+num2str(JMcolortable_B[index])+")"	return sEnd