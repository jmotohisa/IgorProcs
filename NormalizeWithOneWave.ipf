Macro NormalizeAll(instance,orig)	String instance="wave",orig=WaveList(instance+"*",";","")	Prompt instance,"instance"	Prompt orig,"wave name for standard",popup,WaveList(instance+"*",";","")		PauseUpdate; Silent 1	String wv0,wavelists = WaveList(instance+"*",";","")	Variable index=0		do		wv0=GetStrFromList(wavelists,index,";")		if(strlen(wv0)==0)			break		endif		if(cmpstr(wv0, orig) !=0)			$wv0 /=$orig-1		endif		index+=1	while(1)	$orig /=$orig-1	End MacroMacro NormalizeAllOnParticularX(graphname,npoint)	String graphname	Variable npoint=0	Prompt graphname,"Graph Name",popup,WinList("*",";","WIN:1")	Prompt npoint,"number of point"		PauseUpdate; Silent 1	String wv0,wavelists = WaveList("*",";","WIN:"+graphname)	Variable index=0,norm		do		wv0=GetStrFromList(wavelists,index,";")		if(strlen(wv0)==0)			break		endif		norm = $wv0[npoint]		$wv0 /=norm		index+=1	while(1)End Macro