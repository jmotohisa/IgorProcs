#pragma rtGlobals=1		// Use modern global access method.// JMGraphStyles.ipf////	revision history//		?/?/?		ver 0.1	first version (named as loadSPEdata.ipf)//		17/07/26	ver 0.2	add Current-VoltageProc Wavelength(vert) : GraphStyle	Variable vert=1	Prompt vert, "with vertical scale",popup,"yes;no"	PauseUpdate; Silent 1		// modifying window...	ModifyGraph/Z tick=2	ModifyGraph/Z mirror=1	ModifyGraph/Z lblMargin(left)=12,lblMargin(bottom)=8	ModifyGraph/Z lblLatPos(left)=-6//	Label/Z left "\\F'Times New Roman'Intensity (arb units)"//	Label/Z bottom "\\F'Times New Roman'Wavelength (nm)"	Label/Z left "Intensity (arb. units)"	Label/Z bottom "Wavelength (nm)"	ModifyGraph fSize(bottom)=16      ModifyGraph fSize=16      if(vert==2)      	ModifyGraph noLabel(left)=1      else 		ModifyGraph noLabel(left)=0	      EndifEndMacroProc Energy_eV(vert) : GraphStyle	Variable vert=1	Prompt vert, "with vertical scale",popup,"yes;no"	PauseUpdate; Silent 1		// modifying window...	ModifyGraph/Z tick=2	ModifyGraph/Z mirror=1	ModifyGraph/Z lblMargin(left)=12,lblMargin(bottom)=8	ModifyGraph/Z lblLatPos(left)=-6//	Label/Z left "\\F'Times New Roman'Intensity (arb units)"//	Label/Z bottom "\\F'Times New Roman'Wavelength (nm)"	Label/Z left "Intensity (arb. units)"	Label/Z bottom "Energy (eV)"	ModifyGraph fSize(bottom)=16       ModifyGraph fSize=16      if(vert==2)      	ModifyGraph noLabel(left)=1      else 		ModifyGraph noLabel(left)=0	      EndifEndMacroProc Current_Voltage(flog) : GraphStyle	Variable flog=1	Prompt flog, "log scale",popup,"yes;no"	PauseUpdate; Silent 1		// modifying window...	ModifyGraph/Z tick=2	ModifyGraph/Z mirror=1	ModifyGraph/Z lblMargin(left)=12,lblMargin(bottom)=8	ModifyGraph/Z lblLatPos(left)=-6//	Label/Z left "\\F'Times New Roman'Intensity (arb units)"//	Label/Z bottom "\\F'Times New Roman'Wavelength (nm)"	Label/Z left "Current (\\U)"	Label/Z bottom "Voltage (\\U)"	ModifyGraph fSize(bottom)=16	ModifyGraph fSize=16	if(flog==2)		ModifyGraph log(left)=1	EndifEndMacroProc Power_IntegInt(vert,flog) : GraphStyle	Variable vert=1,flog=1	Prompt vert, "with vertical scale",popup,"yes;no"	Prompt flog, "log scale",popup,"yes;no"	PauseUpdate; Silent 1		// modifying window...	ModifyGraph/Z tick=2	ModifyGraph/Z mirror=1	ModifyGraph/Z lblMargin(left)=12,lblMargin(bottom)=8	ModifyGraph/Z lblLatPos(left)=-6//	Label/Z left "\\F'Times New Roman'Intensity (arb units)"//	Label/Z bottom "\\F'Times New Roman'Wavelength (nm)"	Label/Z bottom "Excitation Intensity (\\U)"	Label/Z left "Integrated Intensity (arb. units)"	ModifyGraph fSize(bottom)=16   ModifyGraph fSize=16   if(vert==2)   	ModifyGraph noLabel(left)=1   else 		ModifyGraph noLabel(left)=0	   Endif	if(flog==2)		ModifyGraph log(left)=1		ModifyGraph log(bottom)=1	EndifEndMacroProc Current_IntegInt(vert,flog) : GraphStyle	Variable vert=1,flog=1	Prompt vert, "with vertical scale",popup,"yes;no"	Prompt flog, "log scale",popup,"yes;no"	PauseUpdate; Silent 1		// modifying window...	ModifyGraph/Z tick=2	ModifyGraph/Z mirror=1	ModifyGraph/Z lblMargin(left)=12,lblMargin(bottom)=8	ModifyGraph/Z lblLatPos(left)=-6//	Label/Z left "\\F'Times New Roman'Intensity (arb units)"//	Label/Z bottom "\\F'Times New Roman'Wavelength (nm)"	Label/Z bottom "Current (\\U)"	Label/Z left "Integrated Intensity (arb. units)"	ModifyGraph fSize(bottom)=16   ModifyGraph fSize=16   if(vert==2)   	ModifyGraph noLabel(left)=1	else		ModifyGraph noLabel(left)=0		Endif	if(flog==2)		ModifyGraph log(left)=1		ModifyGraph log(bottom)=1	EndifEndMacroProc Current_ELInt(vert,flog) : GraphStyle	Variable vert=1,flog	Prompt vert, "with vertical scale",popup,"yes;no"	Prompt flog, "log scale",popup,"yes;no"	PauseUpdate; Silent 1		// modifying window...	ModifyGraph/Z tick=2	ModifyGraph/Z mirror=1	ModifyGraph/Z lblMargin(left)=12,lblMargin(bottom)=8	ModifyGraph/Z lblLatPos(left)=-6//	Label/Z left "\\F'Times New Roman'Intensity (arb units)"//	Label/Z bottom "\\F'Times New Roman'Wavelength (nm)"	Label/Z bottom "Current (\\U)"	Label/Z left "EL Intensity (arb. units)"	ModifyGraph fSize(bottom)=16	ModifyGraph fSize=16	if(vert==2)		ModifyGraph noLabel(left)=1	else		ModifyGraph noLabel(left)=0		Endif	if(flog==2)		ModifyGraph log(left)=1		ModifyGraph log(bottom)=1	EndifEndMacroProc ChangeScaleUnitWL(wname)	String wname	ChangeScaleUnit(wname,"m","V")EndProc ChangeScaleUnit(wname,xunit,yunit)	String wname,xunit,yunit	PauseUpdate;Silent 1	Variable sc1,sc2	sc1=DimOffset($wname,0)	sc2=DimDelta($wname,0)	SetScale/P x sc1,sc2,xunit, $wname	SetScale d 0,1,yunit, $wnameEnd