// Colorwave.ipf// Aug 28  2003Macro ColorWaves() : GraphStyle	Silent 1;PauseUpdate	Modify/Z lSmooth=1,lHair=0.5,minor(bottom)=1	Modify/Z rgb[0]=(65535,0,0)	Modify/Z rgb[1]=(0,0,65535),rgb[2]=(3009,65535,1882),rgb[3]=(0,0,0)	Modify/Z rgb[4]=(0,0,65535),rgb[5]=(63953,3661,65535)	Modify/Z rgb[6]=(37510,1,1),rgb[7]=(27232,40528,22540),rgb[8]=(1531,1314,28456)	Legend/C/N=default ""EndMacro ColorWaves2() : GraphStyle	Silent 1;PauseUpdate	Modify/Z rgb[0]=(65535,0,0)	Modify/Z rgb[1]=(0,0,65535),rgb[2]=(3009,65535,1882),rgb[3]=(0,0,0)	Modify/Z rgb[4]=(0,0,65535),rgb[5]=(63953,3661,65535)	Modify/Z rgb[6]=(37510,1,1),rgb[7]=(27232,40528,22540),rgb[8]=(1531,1314,28456)End