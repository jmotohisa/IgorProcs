Macro AppendDummyDataAll(grname,xmin0,xmax0)|| Append dummy data to all the waves displayed in the graph| to make scales consistent for the construction of matrix data|	Variable xmin0,xmax0	String grname	PauseUpdate; Silent 1		Variable index = 0	String wlist = WaveList("*",";","WIN:"+grname)	String w0	do		w0=GetStrFromList(wlist,index,";")		if(strlen(w0)==0)			break		endif		Trimfor3Dplot(w0,xmin0,xmax0)		index+=1	while(1)EndMacroMacro AppendDummyData(w0,xmin0,xmax0)	String w0	Variable xmin0,xmax0	PauseUpdate; Silent 1		Variable xmin,xmax,npnt,dx,nnmin,nnmax	WaveStats/Q $w0	npnt = V_npnts	xmin=pnt2x($w0,0)	dx=pnt2x($w0,1)-xmin	xmax=pnt2x($w0,npnt)		nnmax = (xmax0-xmax)/dx	if(nnmax>0)		InsertPoints npnt+1,nnmax,$w0		xmax = xmax0	endif		nnmin = (xmin-xmin0)/dx	if(nnmin>0) 		InsertPoints 0,nnmin, $w0		xmin = xmin0	endif		SetScale/I x,xmin,xmax,"V",$w0EndMacro